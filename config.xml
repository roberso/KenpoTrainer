<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gmail.roberson.kenn.kenpoTrainerCordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>KenpoTrainer</name>
    <description>
        Martial Arts Training Tool written in Apache Cordova and JQuery Mobiles
    </description>
    <author email="kenn.roberson@gmail.com" href="">
        Kenn Roberson
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <splash qualifier="port-hdpi" src="res/android/drawable-hdpi/screen.png" />
        <icon qualifier="hdpi" src="res/android/drawable-hdpi/icon.png" />
        <splash qualifier="land-hdpi" src="res/android/drawable-land-hdpi/screen.png" />
        <splash qualifier="land-ldpi" src="res/android/drawable-land-ldpi/screen.png" />
        <splash qualifier="land-mdpi" src="res/android/drawable-land-mdpi/screen.png" />
        <splash qualifier="land-xhdpi" src="res/android/drawable-land-xhdpi/screen.png" />
        <splash qualifier="land-xxhdpi" src="res/android/drawable-land-xxhdpi/screen.png" />
        <splash qualifier="land-xxxhdpi" src="res/android/drawable-land-xxxhdpi/screen.png" />
        <splash qualifier="port-ldpi" src="res/android/drawable-ldpi/screen.png" />
        <icon qualifier="ldpi" src="res/android/drawable-ldpi/icon.png" />
        <splash qualifier="port-mdpi" src="res/android/drawable-mdpi/screen.png" />
        <icon qualifier="mdpi" src="res/android/drawable-mdpi/icon.png" />
        <splash qualifier="port-xhdpi" src="res/android/drawable-xhdpi/screen.png" />
        <icon qualifier="xhdpi" src="res/android/drawable-xhdpi/icon.png" />
        <splash qualifier="port-xxhdpi" src="res/android/drawable-xxhdpi/screen.png" />
        <icon qualifier="xxhdpi" src="res/android/drawable-xxhdpi/icon.png" />
        <splash qualifier="port-xxxhdpi" src="res/android/drawable-xxxhdpi/screen.png" />
        <icon qualifier="xxxhdpi" src="res/android/drawable-xxxhdpi/icon.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <splash src="res/Default@2x~universal~anyany.png" />
        <icon height="20" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-20x20@1x.png" width="20" />
        <icon height="40" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-20x20@2x.png" width="40" />
        <icon height="60" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-20x20@3x.png" width="60" />
        <icon height="29" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-29x29@1x.png" width="29" />
        <icon height="58" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-29x29@2x.png" width="58" />
        <icon height="87" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-29x29@3x.png" width="87" />
        <icon height="80" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-40x40@2x.png" width="80" />
        <icon height="120" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-40x40@3x.png" width="120" />
        <icon height="120" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-60x60@2x.png" width="120" />
        <icon height="180" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-60x60@3x.png" width="180" />
        <icon height="76" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-76x76@1x.png" width="76" />
        <icon height="152" platform="ios" src="res/iOS/AppIcon.appiconset/Icon-76x76@2x.png" width="152" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-texttospeech" spec="^0.1.1" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <engine name="ios" spec="^4.5.5" />
    <engine name="android" spec="^7.1.4" />
    <engine name="browser" spec="^5.0.4" />
</widget>
